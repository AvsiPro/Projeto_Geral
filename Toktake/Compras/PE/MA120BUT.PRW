User Function MA120BUT()

Local aArea := GetArea()
Local aArray := {}   

aadd(aArray,{"BUDGET",   {|| xM120b()},"Aprovacao","Aprovacao" }) //
aadd(aArray,{"BMPORD1",  {|| A120Track() },                "Tracker", "Tracker"})  // 
                  
RestArea(aArea)

Return(aArray)                                              

