User Function AMCCTB03()     

Local lRet 	:= .T.                          
Local aArea	:=	GetArea()


If cEmpAnt <> "10"
	Return(lRet)
EndIf

dbSelectArea("CTT")

DBSetOrder(1)   

If DbSeek(xFilial("CTT") + ALLTRIM(M->C6_CC))
    
   IF CTT->CTT_CLASSE == "1"  
      Alert("Centro de Custo Sintetico"+M->C6_CC+", Utilizar Centro de Custo Analํtico.")        
      lRet := .F.
   EndIf    
                                          
Endif

RestArea(aArea)

Return lRet
