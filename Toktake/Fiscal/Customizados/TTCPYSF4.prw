Static Function NextNum(cCodAtu)

Local lVld 		:= .F.
Local cAux		:= cCodAtu
Local _aArea	:= GetArea()

dbSelectArea("SF4")
dbSetOrder(1)
dbGoTop()

/*---------------------------------------------------------------+
|	Busca o pr๓ximo registro disponํvel na filial de destino.|
+----------------------------------------------------------------*/
While !lVld

	If dbSeek(xFilial("SF4") + AvKey(cAux,"F4_CODIGO"))
		cAux := SOMA1(cAux)
	Else
		lVld := .T.	
	EndIf
  
End


RestArea(_aArea)

Return (cAux)