Static Function SF4Copia(cAlias,nReg,nOpc)

Local aCampos  := {}
Local lCopia := .T.
Private aTELA[0][0],aGETS[0]

/*----------------------------------------------------------------+
|	Envia para processamento dos Gets.							  |
+----------------------------------------------------------------*/
nOpcA := 0

If lCopia
	Begin Transaction
		
		nOpcA := AxInclui( cAlias, nReg, nOpc,,"U_SF4LeReg()",,,,,,,,.T.)
		
		If nOpcA == 1
			MsgInfo("C๓pia concluํda com sucesso!")
		EndIf
			
	End Transaction
EndIf		
			
				
dbSelectArea(cAlias)
	

Return
            


