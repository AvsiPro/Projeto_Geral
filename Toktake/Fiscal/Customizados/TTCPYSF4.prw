User Function CPSF4() 
Local aArea   	:= { Alias(), IndexOrd(), Recno() }
Local nPosSF4 	:= aArea[3]
Local lContinua	:= .T.

If /*xFilial("SF4") # SF4->F4_FILIAL .Or. */(SF4->(EOF()) .and. SF4->(BOF()))
	HELP(" ",1,"ARQVAZIO")
	lContinua := .F.
Endif


If lContinua
	SF4Copia("SF4",nPosSF4,3)
EndIf


Return(.T.)



