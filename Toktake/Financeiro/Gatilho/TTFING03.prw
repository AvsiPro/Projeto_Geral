User Function TTFING03()

Local aArea		:=	GetArea()
Local cRetorno	:=	""
Local cAux		:=	SEE->EE_FAXATU //NossoNum()
Local nTamanho  :=  0
Local cFile     := "\backups\arquivo_"+DTOS(Date())+"_"+SUBS(TIME(),1,2)+"_"+SUBS(TIME(),4,2)+"_"+SUBS(TIME(),7,2)+".txt"
Local nH
 
If SE1->E1_PORTADO == "341" 
    
    cRetorno := SUBS(SE1->E1_NUMBCO,2,8)

Else

	cRetorno	:=	substr(cAux,6,7)+cvaltochar(dig_ver(substr(cAux,6,7)))
	
	If AT(" ",ALLTRIM(cRetorno)) > 0   
	  
		Reclock("SEE",.F.)
		SEE->EE_FAXATU := Strzero(val(SEE->EE_FAXATU)+1,12)
		SEE->(Msunlock())
		
		U_TTFING03()
		
	Endif  
Endif

RestArea(aArea)

Return(cRetorno)                         

