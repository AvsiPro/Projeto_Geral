Static Function dig_ver(cData)
LOCAL L, D, P := 0
L := Len(cdata)
D := 0
P := 1
While L > 0
	P := P + 1
	D := D + (Val(SubStr(cData, L, 1)) * P)
	If P = 9
		P := 1
	End
	L := L - 1
End
D := (mod(D,11)) 
If D == 10
	D := 1
ElseIf D == 0 .Or. D == 1
	D := 0
Else
	D := 11 - D
EndIf
Return(D)