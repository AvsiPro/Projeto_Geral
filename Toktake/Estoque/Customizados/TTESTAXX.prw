Static Function IniCopia(cArmA)

Local aArea := GetArea()
Local aDados := {}
Local nI

// criacao item contabil

// criacao armazem
dbSelectArea("ZZ1")
For nI := 1 To FCount()
	If FieldName(nI) == "ZZ1_COD"
		AADD( aDados, { nI, cArmA } )
	Else
		AADD( aDados, { nI, FieldGet(nI)  } )
	EndIf
Next nI


RecLock("ZZ1",.T.)
For nI := 1 To Len(aDados)               
	FieldPut( aDados[nI][1], aDados[nI][2] )
Next nI
MsUnLock()

MsgInfo("Armaz้m criado!")

RestArea(aArea)
	
Return
            


