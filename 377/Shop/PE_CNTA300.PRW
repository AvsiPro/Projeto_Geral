User Function CNTA300()

Local aParam     := PARAMIXB
Local xRet       := .T.
Local oObj       := ''
Local cIdPonto   := ''
Local cIdModel   := ''
Local lIsGrid    := .F.
Local lOk        := .f. 
Local nPC        := 0
Local dDtVenc    := ctod(" / / ")
Local nQtDias 

If aParam <> NIL
       oObj       := aParam[1]
       cIdPonto   := aParam[2]
       cIdModel   := aParam[3]
       lIsGrid    := ( Len( aParam ) > 3 )

       If cIdPonto == 'FORMLINEPRE'
              If  cIdModel == 'CNFDETAIL'
                     If !lOk
                            nQtDias := MV_PAR02
                            dDtVenc := MV_PAR09
                            lOk := .T.
                     EndIf

                     nPC := aParam[4]
                     
                     If nPC == 1
                            oObj:ADATAMODEL[nPC][1][1][9] := dDtVenc
                     Else
                            oObj:ADATAMODEL[nPC][1][1][9] := MonthSum(dDtVenc,(nPC-1))
                     EndIf                         
              EndIf
       EndIf
EndIf

Return xRet
